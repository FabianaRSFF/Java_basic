package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		
		int m = sc.nextInt();
		int n = sc.nextInt();
		int[][] mat = new int[m][n];
		
		for (int i=0;i<n;i++) {
			for (int j=0;j<n;j++) {
				mat[i][j] = sc.nextInt();
			}
		}
		
		System.out.println();
		
		int x = 8;
		for (int i=0;i<n;i++) {
			for (int j=0;j<n;j++) {
				if (x == 8) {
					System.out.println("Position: " + mat[m][n]);
				}
			}
		
		
		sc.close();

	}

}
}

